{
  "name": "@peggyjs/eslint-parser",
  "version": "0.0.0",
  "description": "ESLint-style parser for Peggy grammars",
  "main": "lib/index.js",
  "scripts": {
    "parser": "peggy -c .peggy.js --plugin ts-pegjs -o src/parser.ts src/parser.peggy",
    "ts": "tsc",
    "lint": "eslint . --ext js,ts",
    "check": "npm run parser && npm run ts && npm run lint",
    "pretest": "npm run check",
    "test": "node --test",
    "coverage": "c8 npm test",
    "ci": "node --test"
  },
  "keywords": [
    "eslint",
    "parser",
    "peg.js",
    "peggy",
    "plugin"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "devDependencies": {
    "@peggyjs/eslint-config": "3.0.0",
    "@types/eslint": "8.4.5",
    "c8": "7.12.0",
    "eslint": "8.20.0",
    "peggy": "2.0.1",
    "ts-pegjs": "2.1.0",
    "typescript": "4.7.4"
  }
}
