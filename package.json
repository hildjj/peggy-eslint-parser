{
  "name": "@peggyjs/eslint-parser",
  "version": "1.2.0",
  "description": "ESLint-style parser for Peggy grammars",
  "main": "lib/index.js",
  "scripts": {
    "parser": "peggy -c .peggy.js --plugin ts-pegjs -o src/parser.ts src/parser.peggy && sed -i '' -e  's/grammarSource\\?: string;/source?: any;/' src/parser.ts",
    "ts": "tsc",
    "ts:sourcemap": "tsc --inlineSourceMap",
    "build": "npm run parser && npm run ts",
    "lint": "eslint . --ext js,ts",
    "test": "mocha",
    "precoverage": "npm run parser && npm run ts:sourcemap",
    "coverage": "c8 npm test",
    "postcoverage": "npm run ts",
    "ci": "npm run coverage"
  },
  "keywords": [
    "eslint",
    "parser",
    "peg.js",
    "peggy",
    "plugin"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "repository": "peggyjs/peggyjs-eslint-parser",
  "bugs": "https://github.com/peggyjs/peggyjs-eslint-parser/issues",
  "devDependencies": {
    "@peggyjs/eslint-config": "3.0.1",
    "@types/eslint": "8.37.0",
    "@types/estree": "1.0.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "c8": "7.13.0",
    "eslint": "8.38.0",
    "eslint-plugin-mocha": "10.1.0",
    "mocha": "10.2.0",
    "peggy": "3.0.2",
    "source-map-support": "0.5.21",
    "ts-pegjs": "4.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=14.20"
  }
}
